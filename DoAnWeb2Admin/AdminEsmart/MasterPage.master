                if ($("#InputThanhPho").val() == "-") {

                    showError("Vui lòng chọn tỉnh hoặc thành phố", $("#InputThanhPho"));
                    loi = true;
                }
                if ($("#InputReal").val() == "") {

                    showError("Vui lòng nhập kết quả", $("#InputReal"));
                    loi = true;
                }
                if ($("#InputPassWord").val() != $("#InputCfmPassWord").val()) {
                    showError("Mật khẩu không trùng khớp", $("#InputPassWord"));
                    showError("Mật khẩu không trùng khớp", $("#InputCfmPassWord"));
                    loi = true;
                }
                if (loi) return;

                var ngay = $("#InputNgaySinh").val();
                var thang = $("#InputThangSinh").val();
                var nam = $("#InputNamSinh").val();
                var action = "signup";
                var username = $("#InputUserName").val();
                var password = $("#InputPassWord").val();
                var fullname = $("#InputName").val();
                var birthday = nam + "-" + thang + "-" + ngay;
                var sex = $("#InputNGioiTinh").val();
                var address = $("#InputDiaChi").val();
                var city = $("#InputThanhPho").val();
                var email = $("#InputEmail").val();
                var phone = $("#InputPhone").val();



                var data = { "action": action, "username": username, "password": password, "fullname": fullname, "birthday": birthday, "sex": sex, "address": address, "city": city, "email": email, "phone": phone };

                $.getJSON(urlAjax, data)

                  .done(function (response) {
                      console.log("JSON Data: " + response);
                      if (response.status == false) {
                          showError("Tài khoản '" + username + "' đã tồn tại trong hệ thống", $("#InputUserName"));
                          $("#InputUserName").removeClass("error");
                      }
                      else {
                          if (window.location.hash.toString() == '#payment') {
                              window.location = "//" + location.host + "/checkout.aspx";
                          }
                          else {
                              window.location.reload();
                          }
                      }
                  })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      console.log("Request Failed: " + err);
                      alert("Secer bị lỗi! vui lòng quay lại sau :)");
                  });
            }

