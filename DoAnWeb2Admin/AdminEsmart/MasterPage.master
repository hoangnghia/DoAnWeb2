        <script type="text/javascript">


            // $("#lbLoi").hide();        
            function showError(errText, ctr) {
                ctr.parent().next().removeClass("hide").html(errText);
                ctr.addClass("error");
            }
            function sendContact() {
                var name = $("#InputNameContact").val();
                var email = $("#InputEmail").val();
                var title = $("#InputTitleContact").val();
                var content = $("#msgContact").val();

                if (name == "") {
                    $("#lbLoiContact").html("Vui lòng nhập họ tên!")
                    $("#InputNameContact").focus();
                    return;
                }
                if (email == "") {
                    $("#lbLoiContact").html("Vui lòng nhập email!")
                    $("#InputEmail").focus();
                    return;
                }
                if (!isValidEmailAddress(email)) {
                    $("#lbLoiContact").html("Email không hợp lệ.....");
                    $("#InputEmail").focus();
                    return;
                }
                if (title == "") {
                    $("#lbLoiContact").html("Vui lòng nhập tiêu đề!")
                    $("#InputTitleContact").focus();
                    return;
                }
                if (content == "") {
                    $("#lbLoiContact").html("Vui lòng nhập nội dung muốn gửi!")
                    $("#msgContact").focus();
                    return;
                }
                if (!isValidEmailAddress(email)) {
                    $("#lbLoi").html("Email không hợp lệ.....");
                    $("#InputEmail").focus();
                    return;
                }
                var action = "sendcontact";

                var data = { "action": action, "name": name, "email": email, "title": title, "content": content };

                $.getJSON(urlAjax, data)

                  .done(function (response) {
                      console.log("JSON Data: " + response);
                      if (response.status == false) {
                          alert("Có lỗi xảy ra!");
                      }
                      else {
                          $("#lbLoiContact").hide();
                          $("#sendContact").modal("show");
                      }
                  })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      console.log("Request Failed: " + err);
                      alert("Server bi loi, vui long thu lai sau :)");
                  });
            }
            function login() {

                var loi = false;

                $(".lbLoi").addClass("hide");
                $("#aspnetForm input").removeClass("error");

                if ($("#InputUserNameDangNhap").val() == "") {

                    showError("Vui lòng nhập tài khoản", $("#InputUserNameDangNhap"));
                    loi = true;
                }
                if ($("#InputPassWordDangNhap").val() == "") {

                    showError("Vui lòng nhập mật khẩu", $("#InputPassWordDangNhap"));
                    loi = true;
                }


                if (loi) return;

                var username = $("#InputUserNameDangNhap").val();
                var password = $("#InputPassWordDangNhap").val();

                var action = "login";

                var data = { "action": action, "username": username, "password": password };

                $.getJSON(urlAjax, data)

                  .done(function (response) {
                      console.log("JSON Data: " + response);
                      if (response.status == false) {
                          showError("Tài khoản hoặc mật khẩu không đúng! Vui lòng kiểm tra lại", $("#InputPassWordDangNhap"));
                          $("#InputPassWordDangNhap").removeClass("error");
                      }
                      else {
                          if (window.location.hash.toString() == '#payment') {
                              window.location = "//" + location.host + "/checkout.aspx";
                          }
                          else {
                              window.location.reload();
                          }
                          //window.location = "/"; //window.location.reload();
                      }
                  })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      console.log("Request Failed: " + err);
                      alert("Server bi loi, vui long thu lai sau :)");
                  });
            }

            function checkpayment() {

                if ($("#orDerNhanHang").is(":checked") == false && $("#orDerNganHang").is(":checked") == false) {
                    alert("Bạn chưa chọn phương thức thanh toán");
                    return;
                }


                var loi = false;

                // Lấy ngày giờ hệ thống
                var fullDate = new Date();
                var twoDigitMonth = fullDate.getMonth() + ""; if (twoDigitMonth.length == 1) twoDigitMonth = "0" + twoDigitMonth;
                var twoDigitDate = fullDate.getDate() + ""; if (twoDigitDate.length == 1) twoDigitDate = "0" + twoDigitDate;
                var currentDate = fullDate.getFullYear() + "/" + twoDigitMonth + "/" + twoDigitDate;


                var MaND = $("#MaND").html();
                var TongTien = $("#TongTien").html();
                var NgayLap = currentDate;
                var MaSP = $("#MaSanPham").html();
                var DonGia = $("#Gia").html();
                var ThanhTien = $("#ThanhTien").html();
                var SoLuong = $("#SoLuong").html();
                var LoaiThanhToan = $("input[name='group2']:checked").val();
                var action = "saveOrder";

                var data = { "action": action, "mand": MaND, "tongtien": TongTien, "ngaylap": NgayLap, "masp": MaSP, "dongia": DonGia, "thanhtien": ThanhTien, "soluong": SoLuong, "loaithanhtoan": LoaiThanhToan };

                $.getJSON(urlAjax, data)

                  .done(function (response) {
                      console.log("JSON Data: " + response);
                      if (response.status == false) {

                      }
                      else {
                          window.location = "//" + location.host + "/payment.aspx";

                          //window.location = "/"; //window.location.reload();
                      }
                  })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      console.log("Request Failed: " + err);
                      alert("Server bi loi, vui long thu lai sau :)");
                  });
            }
            function changePassWord() {


                var mk = $("#password1").val();
                var mk2 = $("#password2").val();
                if ($("#password1").val().length < 8) {
                    $("#password1").focus();
                    return;
                }
                if ($("#password1").val() == "") {
                    $("#password1").focus();
                    return;
                }
                if ($("#password2").val() == "") {
                    $("#password2").focus();
                    return;
                }
                if ($("#password1").val() != $("#password2").val()) {
                    return;
                }

                var action = "changepassword";

                var data = { "action": action, "password": mk };

                $.getJSON(urlAjax, data)

                  .done(function (response) {
                      console.log("JSON Data: " + response);
                      if (response.status == false) {
                          alert("Có lỗi xảy ra");

                      }
                      else {
                          $("#changepassword_modal").modal("show");

                          //window.location = "/"; //window.location.reload();
                      }
                  })
                  .fail(function (jqxhr, textStatus, error) {
                      var err = textStatus + ", " + error;
                      console.log("Request Failed: " + err);
                      alert("Server bi loi, vui long thu lai sau :)");
                  });
            }

