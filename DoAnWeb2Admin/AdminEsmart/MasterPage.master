            function showError(errText, ctr) {
                ctr.parent().next().removeClass("hide").html(errText);
                ctr.addClass("error");
            }
            // Kiểm Tra Email
            function isValidEmailAddress(emailAddress) {
                var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
                return pattern.test(emailAddress);
            };

            function signup() {

                var loi = false;

                $(".lbLoi").addClass("hide");
                $("#aspnetForm input").removeClass("error");
                $("#aspnetForm select").removeClass("error");
                if ($("#InputUserName").val() == "") {

                    showError("Vui lòng nhập tài khoản", $("#InputUserName"));
                    loi = true;
                }
                if ($("#InputPassWord").val() == "") {

                    showError("Vui lòng nhập mật khẩu", $("#InputPassWord"));
                    loi = true;
                }
                //if ($("#InputEmail").val() == "") {

                //    showError("Vui lòng nhập email", $("#InputEmail"));
                //    loi = true;
                //}               
                if (!isValidEmailAddress($("#InputEmail").val())) {
                    showError("Email không hợp lệ", $("#InputEmail"));
                    loi = true;
                }
                if ($("#InputDiaChi").val() == "") {

                    showError("Vui lòng nhập địa chỉ", $("#InputDiaChi"));
                    loi = true;
                }
                if ($("#InputName").val() == "") {

                    showError("Vui lòng nhập họ tên", $("#InputName"));
                    loi = true;
                }

                if ($("#InputPhone").val() == "") {

                    showError("Vui lòng số điện thoại", $("#InputPhone"));
                    loi = true;
                }
                if ($("#InputCfmPassWord").val() == "") {

                    showError("Vui lòng nhập lại mật khẩu", $("#InputCfmPassWord"));
                    loi = true;
                }
                if ($("#InputNamSinh").val() == "-") {

                    showError("Vui lòng ngày tháng năm sinh", $("#InputNamSinh"));
                    loi = true;
                }
                if ($("#InputThangSinh").val() == "-") {

                    showError("Vui lòng ngày tháng năm sinh", $("#InputThangSinh"));
                    loi = true;
                }
                if ($("#InputNgaySinh").val() == "-") {

                    showError("Vui lòng ngày tháng năm sinh", $("#InputNgaySinh"));
                    loi = true;
                }
                if ($("#InputNGioiTinh").val() == "-") {

                    showError("Vui lòng chọn giới tính", $("#InputNGioiTinh"));
                    loi = true;
